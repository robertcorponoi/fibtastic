{
  "name": "fibtastic",
  "version": "2.0.0",
  "description": "Calculate a Fibonacci sequence up to a certain number of integers or to get just one specific value from anywhere in the sequence.",
  "main": "index.js",
  "module": "fibtastic.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c --watch",
    "test": "rollup -c && mocha"
  },
  "keywords": [
    "fibonacci",
    "calculate",
    "sequence",
    "number",
    "numbers",
    "value",
    "values"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robertcorponoi/fibtastic"
  },
  "author": "Robert Corponoi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "typescript": "^3.3.3"
  }
}
