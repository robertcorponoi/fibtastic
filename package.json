{
  "name": "fibtastic",
  "version": "2.1.0",
  "description": "Calculate a Fibonacci sequence up to a certain number of integers or to get just one specific value from anywhere in the sequence",
  "main": "index.js",
  "module": "fibtastic.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c --watch",
    "test": "ava"
  },
  "keywords": [
    "fibonacci",
    "calculate",
    "sequence",
    "number",
    "numbers",
    "value",
    "values",
    "fibtastic",
    "calc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robertcorponoi/fibtastic"
  },
  "author": "Robert Corponoi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "ava": "^3.5.0",
    "rollup": "^2.0.5",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^3.8.3"
  }
}
